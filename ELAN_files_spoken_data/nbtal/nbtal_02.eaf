        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a91" TIME_SLOT_REF1="ts175" TIME_SLOT_REF2="ts176">
                <ANNOTATION_VALUE>this new kingdom-centric Scottish identity</ANNOTATION_VALUE>
