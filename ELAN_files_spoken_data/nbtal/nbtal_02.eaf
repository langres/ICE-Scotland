        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a473" TIME_SLOT_REF1="ts297" TIME_SLOT_REF2="ts298">
                <ANNOTATION_VALUE>unfortunately</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a145" TIME_SLOT_REF1="ts299" TIME_SLOT_REF2="ts300">
                <ANNOTATION_VALUE>this account to the beginning of modern Scottish identity</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
